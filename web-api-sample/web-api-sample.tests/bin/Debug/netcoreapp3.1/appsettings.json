{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Path": {
    "ElasticSearch": "__Path__"
  },
  "JwtIssuerOptions": {
    "Issuer": "__Issuer__",
    "Audience": "__Audience__",
    "Secret": "__SecretKey__"
  },
  "ConnectionStrings": {
    "MysqlConnection": "__MySql.ConnectionString__"
  },
  "ExternalServices": {
    "Email": {
      "PrimaryDomain": "__Domain__",
      "PrimaryPort": "__Port__",
      "UsernameEmail": "__Email__",
      "UsernamePassword": "__Password__"
    }
  }
}